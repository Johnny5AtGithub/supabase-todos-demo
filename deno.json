{
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "checkJs": false
  },
  "exclude": ["Test/", "*.k6.js"],
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false,
    "proseWrap": "preserve"
  }
}
